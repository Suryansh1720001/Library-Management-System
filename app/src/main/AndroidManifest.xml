<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">


    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.LibraryManagementSystem"
        tools:targetApi="31">
        <activity android:name="com.example.librarymanagementsystem.UserReissueBook"></activity>
        <activity android:name="com.example.librarymanagementsystem.AdminReissueBook" />
        <activity android:name="com.example.librarymanagementsystem.SearchBookSet" />
        <activity android:name="com.example.librarymanagementsystem.SearchBook" />
        <activity android:name="com.example.librarymanagementsystem.AdminCollectFine" />
        <activity android:name="com.example.librarymanagementsystem.UserSeeMyBooks" />
        <activity android:name="com.example.librarymanagementsystem.AdminReturnBook" />
        <activity android:name="com.example.librarymanagementsystem.AdminIssueBook" />
        <activity android:name="com.example.librarymanagementsystem.AdminUpdateBook" />
        <activity android:name="com.example.librarymanagementsystem.AdminRemoveBook" />
        <activity android:name="com.example.librarymanagementsystem.AdminAddBook" />
        <activity android:name="com.example.librarymanagementsystem.UserHome" />
        <activity android:name="com.example.librarymanagementsystem.AdminHome" />
        <activity android:name="com.example.librarymanagementsystem.SignUpActivity" />
        <activity android:name="com.example.librarymanagementsystem.SignInActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>